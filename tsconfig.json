{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./*"],
      "@server/*": ["./src/*"],
      "@db/*": ["./src/db/*"],
      "@selfTypes/*": ["./types/*"],
      "@services/*": ["./src/services/*"],
      "@client/*": ["./src/client/*"]
    },
    "types": [
      "@cloudflare/workers-types/2023-07-01",
      "@types/node",
      "vite/client",
      "typed-htmx",
      "vitest/globals",
      "@cloudflare/workers-types/experimental",
      "@cloudflare/vitest-pool-workers"
    ],
    "strict": true,
    "skipLibCheck": true
  }
}
