{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "types": [
      "@cloudflare/workers-types/2023-07-01",
      "@types/node",
      "@types/jsonwebtoken",
      "vite/client",
      "typed-htmx"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "@server/*": ["./src/*"],
      "@db/*": ["./src/db/*"],
      "@selfTypes/*": ["./types/*"],
      "@services/*": ["./src/services/*"],
      "@client/*": ["./src/client/*"]
    }
  }
}
